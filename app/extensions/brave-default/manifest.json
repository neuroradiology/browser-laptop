{
  "name": "brave-default",
  "manifest_version": 2,
  "version": "1.0",
  "content_scripts": [{
    "all_frames": true,
    "matches": ["http://*/*", "https://*/*"],
    "run_at": "document_start",
    "js": [
      "brave-default.js"
    ],
    "css": [
      "brave-default.css"
    ]
  }],
  "incognito": "spanning"
}
